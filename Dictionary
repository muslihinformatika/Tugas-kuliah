#dictonary
identitas = {
'nama' : 'muslih',
'usia' : 19,
'status' : 'mahasiswa'
}

#menampilkan dictionary menggunakan perulangan
for i in identitas.items():
	print(i)
	
for i in identitas:
	print(i)
	
for i in identitas.values():
	print(i)
	
#mengupdate salah satu dictionary
identitas ['usia'] = 21
print(identitas)


#menghapus dictionary
identitas.pop('status')
print(identitas)

print(identitas.popitem())

identitas.clear()
print(identitas)

#menambahkan dictionary
identitas['asal'] = 'sendana'
print(identitas)